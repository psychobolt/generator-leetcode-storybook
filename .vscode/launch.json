{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Tests",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "yarn",
        "stopOnEntry": false,
        "args": [
          "--runInBand",
          "--no-cache"
        ],
        "cwd": "${workspaceRoot}",
        "runtimeArgs": [
          "test",
        ],
        "console": "integratedTerminal",
      },
      {
        "name": "Debug Main Generator",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "yarn",
        "stopOnEntry": false,
        "args": [
          "../"
        ],
        "cwd": "${workspaceRoot}/tmp",
        "runtimeArgs": [
          "yo",
        ],
        "console": "integratedTerminal",
      }
    ],
  }